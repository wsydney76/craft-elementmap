{%- do view.registerAssetBundle("wsydney76\\elementmap\\assets\\ElementMapBundle") -%}

<div class="elementmap">
	{% if elements is iterable %}
		<ul>
			{% for element in elements %}
				<li>
					<a target="_blank" href="{{ element.url }}" data-id="{{ element.id }}" data-editable><span class="icon icon-mask">{{ svg(element.icon) }}</span>{{ element.title }}</a>
				</li>
			{% endfor %}
		</ul>
	{% elseif elements %}
		{{ elements }}
	{% else %}
		<span>--</span>
	{% endif %}

</div>
